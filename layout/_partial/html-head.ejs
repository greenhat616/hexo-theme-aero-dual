<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="<%= page.description || '' %>">
  <meta name="author" content="<%= config.author || '' %>">
  <!-- Open Graph Data -->
  <meta property="og:title" content="<%= page.title || config.title || '' %>"/>
  <meta property="og:description" content="<%= page.description || config.description || '' %>" />
  <meta property="og:site_name" content="<%= config.title || '' %>"/>
  <meta property="og:type" content="<%= page.title ? 'article' : 'website' %>" />
  <meta property="og:image" content="<%= config.url %><%= config.cover %>"/>
  <% if (theme.rss) { %>
    <link rel="alternate" href="<%- theme.rss %>" title="<%= config.title %>" type="application/atom+xml">
  <% } %>
  <% if (theme.favicon) { %>
    <link rel="icon" href="<%- theme.favicon %>">
  <% } %>

  <!-- Site Title -->
  <title><% if (page.title){ %><%= page.title %> | <% } %><%- config.title %></title>

  <!-- APP CSS -->
  <% var targetStyle = 'css/style.' + (config.theme_style || 'light') + '.css' %>
  <%- css(targetStyle) %>

  <!-- Google Analytics -->
  <%- partial('google-analytics') %>
  <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<script>
  (adsbygoogle = window.adsbygoogle || []).push({
    google_ad_client: "ca-pub-2589969775596619",
    enable_page_level_ads: true
  });
</script>
<style>
  .v a {
    border-bottom: 0;
  }
</style>
</head>
